{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "ID": {
      "type": "string"
    },
    "audit": {
      "type": ["object", "null"],
      "properties": {
        "Created at": {
          "type": "string",
          "format": "date-time",
          "example": "2016-07-21T17:32:28Z"
        },
        "Last modified at": {
          "type": "string",
          "format": "date-time",
          "example": "2018-07-21T17:32:28Z"
        },
        "Created by": {
          "type": "string"
        },
        "Last modified by": {
          "type": "string"
        }
      }
    }
  }
}
